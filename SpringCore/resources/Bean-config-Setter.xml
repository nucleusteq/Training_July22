<?xml version = "1.0" encoding = "UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
   http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">
   <bean id="employee" class="com.mt.training.SetterInjection.Employee">
      <property name="id" value="1"></property>
      <property name="name" value="Parth"></property>
   </bean>
   <bean name="office" class="com.mt.training.SetterInjection.Office">
      <property name="name" value="Nucleusteq"></property>
      <property name="employee" ref="employee"></property>
   </bean>
</beans>